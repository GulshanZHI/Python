#Question 01
x = input()
y = input()
z = int(x)+int(y)
m = float(x)+float(y)
n= str(x)+str(y)
print("The addition result is = ", z)
print(f"The addition result is =  {z}")
print("The addition result is = ", m)
print("The addition result is = ", n)

#Question 02

x = input("Enter first Integer value = ")
y = input("Enter second Integer value = ")
z = int(x)+int(y)
print("The addition result is = ", z)
print(f"The addition result is =  {z}")

